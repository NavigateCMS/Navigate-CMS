{
    "name": "jycr753/ip-utils",
    "replaces": "xrstf/ip-utils",
    "description": "small library to deal with IPv4 & IPv6 addresses and subnets",
    "keywords": [
        "ip",
        "ipv4",
        "ipv6",
        "subnet",
        "matching"
    ],
    "authors": [
        {
            "name": "Christoph Mewes",
            "homepage": "http://www.xrstf.de/"
        },
        {
            "name": "Jorge Y. C. Rodriguez",
            "email": "self@jorgerodriguez.dk"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "IpUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.4.2"
    }
}
