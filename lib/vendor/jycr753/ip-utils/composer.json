{
    "name": "jycr753/ip-utils",
    "description": "small library to deal with IPv4 & IPv6 addresses and subnets",
    "keywords": [
        "ip",
        "ipv4",
        "ipv6",
        "subnet",
        "matching"
    ],
    "authors": [
        {
            "name": "Christoph Mewes",
            "homepage": "http://www.xrstf.de/"
        }
    ],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=5.5.0"
    },
    "autoload": {
        "psr-4": {
            "IpUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.26"
    }
}
